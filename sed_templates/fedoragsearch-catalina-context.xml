<Context path="/fedoragsearch">
	<Resources className="org.apache.naming.resources.VirtualDirContext"
		extraResourcePaths="/WEB-INF/classes/fgsconfigFinal=${FEDORA_HOME}/gsearch/fgsconfigFinal"
		/>
	<Loader className="org.apache.catalina.loader.VirtualWebappLoader"
		virtualClasspath="${FEDORA_HOME}/gsearch/fgsconfigFinal"
		/>
	<JarScanner scanAllDirectories="true" />
</Context>
